.Contenido
	background green
	width 100%
	display: flex
	flex-direction column
	&-Skill
		background-image url(../img/paisaje.jpg)
		background-size counter-reset 
		min-height 6em
		display flex
		&-info
			width 100%
			background: rgba(231,56,39,0);
			background: -moz-linear-gradient(left, rgba(231,56,39,0) 0%, rgba(240,47,23,0.29) 29%, rgba(246,41,12,0.49) 49%, rgba(241,111,92,0.88) 88%, rgba(248,80,50,1) 100%);
			background: -webkit-gradient(left top, right top, color-stop(0%, rgba(231,56,39,0)), color-stop(29%, rgba(240,47,23,0.29)), color-stop(49%, rgba(246,41,12,0.49)), color-stop(88%, rgba(241,111,92,0.88)), color-stop(100%, rgba(248,80,50,1)));
			background: -webkit-linear-gradient(left, rgba(231,56,39,0) 0%, rgba(240,47,23,0.29) 29%, rgba(246,41,12,0.49) 49%, rgba(241,111,92,0.88) 88%, rgba(248,80,50,1) 100%);
			background: -o-linear-gradient(left, rgba(231,56,39,0) 0%, rgba(240,47,23,0.29) 29%, rgba(246,41,12,0.49) 49%, rgba(241,111,92,0.88) 88%, rgba(248,80,50,1) 100%);
			background: -ms-linear-gradient(left, rgba(231,56,39,0) 0%, rgba(240,47,23,0.29) 29%, rgba(246,41,12,0.49) 49%, rgba(241,111,92,0.88) 88%, rgba(248,80,50,1) 100%);
			background: linear-gradient(to right, rgba(231,56,39,0) 0%, rgba(240,47,23,0.29) 29%, rgba(246,41,12,0.49) 49%, rgba(241,111,92,0.88) 88%, rgba(248,80,50,1) 100%);
